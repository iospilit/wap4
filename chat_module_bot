{# word layout #}
{% macro xepchu(doc,bot) %}
{% from '_functions' import get,add,login,up %}
{% import '_functions' as func %}
{% set login = login()|trim %}
{% set log = get('user_'~login,'nick') %}
{% set ubot = get_data('user_'~bot)[0].data|json_decode %}
{% set xc = ubot['xc'] %}
{% set countdown = (ubot['xc'].time)-("now"|date("U")) %}
{% set time_answer = 100-countdown %}
{% set xu = 10*countdown %}
{% set list =  {"cau1":"hoa sen","cau2":"nhà trọ","cau3":"nghỉ lễ","cau4":"nghĩa địa","cau5":"địa vị","cau6":"áo phông","cau7":"trường học","cau8":"nhà nghỉ","cau9":"siêu nhân","cau10":"biển Đông","cau11":"tờ giấy","cau12":"hôn nhân","cau13":"gia đình","cau14":"tình dục","cau15":"ngắm gái","cau16":"đẹp trai","cau17":"Thái Bình Dương","cau18":"thủ đô","cau19":"xinh đẹp","cau20":"ngôi nhà","cau21":"rau cải cúc","cau22":"thế giới ảo tình yêu thật","cau23":"thất tình","cau24":"công chúa bong bóng","cau25":"tự hào","cau26":"văn học","cau27":"hòa bình","cau28":"chiến tranh","cau29":"động vật","cau30":"chén cơm","cau31":"chè khoán","cau32":"Hoàng Văn Thụ","cau33":"lịch sử","cau34":"nhân tạo","cau35":"Võ Thị Sáu","cau36":"việt vị","cau37":"hậu môn","cau38":"phố nhỏ","cau39":"Nhật Bản","cau40":"cơ khí","cau41":"con bướm xuân","cau42":"sinh học","cau43":"học sinh","cau44":"Ngô Tất Tố","cau45":"kim loại","cau46":"phi kim","cau47":"mỏ địa chất","cau48":"con sò","cau49":"trứng gà tre","cau50":"yêu thương","cau51":"trẻ con","cau52":"con trâu","cau53":"đường nho","cau54":"đường nhựa","cau55":"nhớ nhung","cau56":"tập viết","cau57":"Nam Cao","cau58":"Xuân Diệu","cau59":"Phan Bội Châu","cau60":"cải cách","cau61":"phát triển","cau62":"Đông Nam Á","cau63":"thông minh","cau64":"sông suối"} %}
{% set rd = random(1..64) %}
{% set key = "cau"~rd %}
{%set text=func.rwurl(list[key])|replace({"-":" "})%}{%set quest=""%} 
{%for i in 0..text|length-1%} 
{%set arr = text|split('')%} 
{%set rand = random(text|length-1)%} 
{%set quest= quest~arr[rand]%} 
{%set text = (arr[:rand]|join('')~arr[(rand+1):]|join(''))%} 
{%endfor%}
{% set end = list[key] %}
{% set raw_end = func.rwurl(end)|replace({"-":" "}) %}
{% set time_start = 'now'|date('U') %}
{% set id = get('xep_chu')|split('@')[19]|trim %}
{% if 'xep chu @'~bot in doc|lower and xc.time < time_start or 'xếp chữ @'~bot in doc|lower and xc.time < time_start %}
{{add('user_'~bot,'xc',{"id":xc.id|trim+1,"time":"now"|date("U")+100,"word":end,"raw_word":raw_end,"end":"no","quest":''~quest~''})}}
{#{{add('xc_'~id,'name',bot)}}
{{add('xc_'~id,'time','now'|date('U'))}}
{{add('xc_'~id,'comment','Hãy sắp xếp các chữ sau thành từ hoặc cụm từ hoàn chỉnh: [b]'~quest~'[/b]')}} 
{{up('xep_chu',id,'up')}}
{{add('user_'~bot,'postguest',get('user_'~bot,'postguest')|trim+1)}}#}
{% endif %}
{% if xc['word'] in doc|lower and xc.time >= time_start or xc['raw_word'] in doc|lower and xc.time >= time_start %}
{% if xc.time > time_start %}
{{add('user_'~login,'xc',get('user_'~login,'xc')|trim+1)}}
{{add('user_'~login,'xu',get('user_'~login,'xu')|trim+xu)}}
{{add('user_'~bot,'xu',get('user_'~bot,'xu')|trim-xu)}}
{{add('xc_'~id,'name',bot)}}
{{add('xc_'~id,'time','now'|date('U'))}}
{{add('xc_'~id,'comment','Chúc mừng @'~login~' đã trả lời đúng câu hỏi vừa rồi trong '~time_answer~' giây ! Bạn nhận được [b]'~xu~'[/b] xu từ @'~bot~' !')}} 
{{up('xep_chu',id,'up') }}
{{add('user_'~bot,'xc',{"id":xc.id|trim,"time":"now"|date("U"),"word":xc.word,"raw_word":xc.raw_word,"end":"yes"})}}
{{add('user_'~bot,'postguest',get('user_'~bot,'postguest')|trim+1)}}
{% endif %}
{% endif %}
{% endmacro %}

{# rock paper scissors #}
{% macro ott(doc,bot) %}
{% from '_functions' import get,add,login,up %}
{% set login = login()|trim %}
{% set log = get('user_'~login,'nick') %}
{% set ubot = get_data('user_'~bot)[0].data|json_decode %}
{% set ott = ubot['ott'] %}
{% set xu = random(50..300) %}
{% set hp = random(5..15) %}
{% set start = random(['bua','keo','bao']) %}
{% set time_start = 'now'|date('U') %}
{% set id = get('guestbook')|split('@')[19]|trim %}
{% if 'ott @'~bot in doc|lower %}
{% set rwdoc = func.rwurl(doc)|replace({'-':' '})|split(' ')|trim %}
{# created #}
{% if ott.time < time_start %}
{{add('user_'~bot,'ott',{"id":ott.id|trim+1,"xu":xu,"player":login,"time":"now"|date("U")+180,"start":start,"end":"no"})}}
{{add('chat_'~id,'name',bot)}}
{{add('chat_'~id,'time','now'|date('U'))}}
{{add('chat_'~id,'comment','@'~login~' tạo kèo thành công! Kèo này trị giá '~xu~' xu!')}} 
{{up('guestbook',id,'up')}}
{% elseif ott.time >= time_start and login!=ott.player and ott.end=='no' %}
{{add('chat_'~id,'name',bot)}}
{{add('chat_'~id,'time','now'|date('U'))}}
{{add('chat_'~id,'comment','@'~login~' từ từ nhé. Em đang chờ @'~ott.player~' bắt kèo :)) @'~ott.player~' ơi, ra chưa dzợ? Còn mỗi '~(ott.time-time_start)~' giây thôi á :( ')}} 
{{up('guestbook',id,'up') }}
{{add('user_'~bot,'postguest',get('user_'~bot,'postguest')|trim+1)}}
{% endif %}
{% elseif 'bat @'~bot in doc|lower %}
{# play #}
{% if ott.time >= time_start and login==ott.player and ott.end=='no' %}
{% if '@'~bot~' '~ott.start|replace({"keo":"bua","bao":"keo","bua":"bao"})|trim in doc|lower %}
{{add('user_'~bot,'ott',{"id":ott.id,"xu":ott.xu,"player":ott.player,"time":"now"|date("U"),"start":ott.start,"end":"yes"})}}
{{add('user_'~login,'xu',get('user_'~login,'xu')|trim+(ott.xu))}}
{{add('user_'~bot,'xu',get('user_'~bot,'xu')|trim-(ott.xu))}}
{{add('chat_'~id,'name',bot)}}
{{add('chat_'~id,'time','now'|date('U'))}}
{{add('chat_'~id,'comment','Bùm [b]'~ott.start~'[/b]. Huhu :menhera15: Em thua òi bùn quá @'~login~' ơi! Được ời thua keo này ta bày keo khác :moew1: @'~login~' đã nhận được '~ott.xu~' xu từ kèo này')}} 
{{up('guestbook',id,'up') }}
{{add('user_'~bot,'postguest',get('user_'~bot,'postguest')|trim+1)}}
{% elseif ('@'~bot~' '~ott.start)|lower in doc|lower %}
{{add('chat_'~id,'name',bot)}}
{{add('chat_'~id,'time','now'|date('U'))}}
{{add('chat_'~id,'comment','Bùm [b]'~ott.start~'[/b]. Ầy zhaa hòa rùi @'~login~' ơi. Chơi lại nào :troll9: ')}} 
{{up('guestbook',id,'up') }}
{{add('user_'~bot,'postguest',get('user_'~bot,'postguest')|trim+1)}}
{% elseif 'apple bao' not in rwdoc or 'apple bua' not in rwdoc or 'apple keo' not in rwdoc %}
{{add('chat_'~id,'name',bot)}}
{{add('chat_'~id,'time','now'|date('U'))}}
{{add('chat_'~id,'comment','Lew lew @'~login~' cái đồ hổng bit chơi :troll8: @'~login~' chỉ được ra [b]bua[/b], [b]keo[/b] hoặc [b]bao[/b] thui à nha :menhera17: ')}} 
{{up('guestbook',id,'up') }}
{{add('user_'~bot,'postguest',get('user_'~bot,'postguest')|trim+1)}}
{% else %}
{{add('user_'~bot,'ott',{"id":ott.id,"xu":ott.xu,"player":ott.player,"time":"now"|date("U"),"start":ott.start,"end":"yes"})}}
{{add('user_'~login,'xu',get('user_'~login,'xu')|trim-(ott.xu))}}
{{add('user_'~bot,'xu',get('user_'~bot,'xu')|trim+(ott.xu))}}
{{add('chat_'~id,'name',bot)}}
{{add('chat_'~id,'time','now'|date('U'))}}
{{add('chat_'~id,'comment','Bùm [b]'~ott.start~'[/b]. Ahihihi!!! Em thắng rồi nha :troll10: @'~login~' đã bị trừ '~ott.xu~' xu vào tài khoản!')}} 
{{up('guestbook',id,'up') }}
{{add('user_'~bot,'postguest',get('user_'~bot,'postguest')|trim+1)}}
{% endif %}

{% endif %}
{% endif %}
{% endmacro %}

